{
  "name": "smart-water-grid-dashboard",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "homepage": "./",
  "scripts": {
    "build": "next build",
    "build:static": "NEXT_EXPORT=true next build",
    "dev": "next dev",
    "dev:full": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "lint": "eslint .",
    "start": "next start",
    "server:dev": "node server/index.js",
    "server:start": "NODE_ENV=production node server/index.js",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:dev:full": "concurrently \"npm run server:dev\" \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "npm run build && electron-builder",
    "electron:build:mac": "npm run build && electron-builder --mac",
    "electron:build:win": "npm run build && electron-builder --win",
    "electron:build:linux": "npm run build && electron-builder --linux",
    "electron:build:static": "npm run build:static && electron-builder",
    "capacitor:init": "npx cap init",
    "capacitor:add:android": "npx cap add android",
    "capacitor:sync": "npm run build:static && npx cap sync",
    "capacitor:open:android": "npx cap open android",
    "android:build": "npm run build:static && npx cap sync && cd android && ./gradlew assembleRelease",
    "android:dev": "npm run build:static && npx cap sync && npx cap open android"
  },
  "dependencies": {
